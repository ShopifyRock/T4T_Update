{% if section.settings.image != blank %}
  <div class="Payment_Options_image_main">
    <img class="faq_banner" src="{{ section.settings.image | img_url: 'master' }}" alt="{{ section.settings.image.alt }}" />
  </div>
{% endif %}

{% schema %}
  {
    "name": "Payment Options",
    "settings": [
	  {
        "type": "image_picker",
        "id": "image",
        "label": "Normal Image"
      }
	]
  }
{% endschema %}